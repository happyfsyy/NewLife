<?xml version="1.0" encoding="utf-8"?>
<com.example.newlife.InterceptLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DispatchAct"
    android:padding="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    android:id="@+id/dispatch_layout">
    <Button
        android:id="@+id/dispatch_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button1"
        android:textAllCaps="false"
        android:layout_margin="10dp"/>
    <ImageView
        android:id="@+id/dispatch_image"
        android:layout_width="100dp"
        android:layout_height="80dp"
        android:layout_margin="10dp"
        android:src="@drawable/apple"
        android:scaleType="centerCrop"/>
</com.example.newlife.InterceptLayout>